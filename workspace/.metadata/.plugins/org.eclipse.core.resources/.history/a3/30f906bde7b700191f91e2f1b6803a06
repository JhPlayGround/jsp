insert into goodsItem values (3,'HDTV',285000,'s사 최고사양','image03.jpg');
insert into goodsItem values (4,'세척기',200000,'s사 최고사양','image04.jpg');
insert into goodsItem values (5,'전자레인지',150000,'s사 최고사양','image05.jpg');
insert into goodsItem values (6,'스타일러',300000,'s사 최고사양','image06.jpg');

select * from goodsItem;

commit work;