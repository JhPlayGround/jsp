-- goodsItem.sql

create table goodsItem
(
	item_id int primary key,
	item_name varchar(50) not null,
	item_price int not null,
	item_des varchar(200),
	item_pic varchar(50)
);

--레코드 설정
insert into goodsItem values (1,'냉장고',850000,'s사 최고사양','image.jpg');
