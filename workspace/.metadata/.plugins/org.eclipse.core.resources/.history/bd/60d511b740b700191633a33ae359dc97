		String sql = "select * from goodsItem";
		
		//query(sql, rowMapper)
		jdbcTemplate.query(sql, new RowMapper<GoodsItemVO>()
		{
			@Override
			public GoodsItemVO mapRow(ResultSet rs, int rowNum) throws SQLException
			{
				// TODO Auto-generated method stub
				return null;
			}
		});
		// TODO Auto-generated method stub
		return null;
	}

}
